{% extends 'base.html.twig' %}

{% block title %}Hello MatchCalendarController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>



<ul>
	{% for match in matches %}
		<li>{{ match.matchtype.name }}</li><br>
        <li>{{ match.localteam }}</li><br>
        <li>{{ match.visitorteam }}</li><br>
        <li>{{ match.date |date("m/d/Y H:i:s") }}</li><br>
        <li>{{ match.duration |date('H:i:s') }}</li><br>
        {# <li>{{ match.stats }}</li> #}
        <li>{% for comp in match.composition['backer'] %}
        Position possible : {{comp}}<br>
        {% endfor %}
        </li>
	{% endfor %}
</ul>



{% endblock %}
